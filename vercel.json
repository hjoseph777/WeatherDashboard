{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "expo start --web",
  "framework": null,
  "functions": {
    "api/weather.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}